{
  "name": "binary/yii2-wallet",
  "description": "Yii2 wallet module",
  "keywords": ["yii2", "wallet", "transactions"],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/asvetliy/math"
    }
  ],
  "authors": [
    {
      "name": "Alex",
      "email": "htc.fail.hell@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0"
  },
  "require-dev": {
    "yiisoft/yii2-faker": "*",
    "yiisoft/yii2-codeception": "*",
    "codeception/verify": "*",
    "codeception/specify": "*",
    "codeception/aspect-mock": "*",
    "binary/math": "dev-staging@dev"
  },
  "autoload": {
    "psr-4": {
      "asmbr\\wallet\\": ""
    }
  },
  "extra": {
    "bootstrap": "asmbr\\wallet\\Bootstrap"
  },
  "config": {
    "secure-http": false
  }
}